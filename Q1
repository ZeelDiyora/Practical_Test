printf("Display the even number between %d and %d are : ",num1,num2);

 for(i=num1,i<=num2,i++){
     r=i%2;
     if(r==0);
      printf("\n%d",i)
  }

printf("\n\nDisplay the odd numbers between %d and %d are : ",num1,num2)
 
  for(i=num1; i<=num2; i++){
        r=i%2;
